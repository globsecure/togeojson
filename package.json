{
  "name": "@mapbox/togeojson",
  "version": "0.16.0",
  "description": "convert KML and GPX to GeoJSON",
  "main": "togeojson.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "eslint togeojson.js && tape test/*.js",
    "cov": "istanbul cover test/index.js && coveralls < ./coverage/lcov.info",
    "browser": "browserify -t brfs test/index.js > test/bundle.js"
  },
  "devDependencies": {
    "brfs": "~2.0.2",
    "coveralls": "~3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.37.0",
    "glob": "^9.3.4",
    "istanbul": "~0.4.4",
    "tape": "~5.6.3"
  },
  "bin": {
    "togeojson": "togeojson"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox/togeojson.git"
  },
  "browser": {
    "xmldom": false
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "license": "BSD-2-Clause",
  "dependencies": {
    "xmldom": "~0.6.0",
    "concat-stream": "~2.0.0",
    "minimist": "1.2.8"
  },
  "keywords": [
    "kml",
    "geojson",
    "gpx",
    "geo",
    "parser",
    "formatter",
    "input",
    "leaflet",
    "maps"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
